# Backend API Configuration
# URL of the backend API server
REACT_APP_API_BASE_URL=http://localhost:8080

# Google Maps Configuration
# Google Maps JavaScript API key for frontend components
# Should have Maps JavaScript API enabled
# Restrict by domain (localhost:3000, yourdomain.com) for security
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Maps Map ID for enhanced map styling (optional)
# Can be created in Google Cloud Console Maps Management
REACT_APP_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id_here

# Sentry Configuration
# Sentry DSN for error tracking and performance monitoring
# Get this from your Sentry project settings
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Application version for release tracking (optional)
# This will be used for Sentry release tracking
REACT_APP_VERSION=1.0.0

# Development Configuration (optional)
# Uncomment for development debugging
# REACT_APP_DEBUG=true

# Build Configuration (optional)
# Uncomment to disable source maps in production builds
# GENERATE_SOURCEMAP=false