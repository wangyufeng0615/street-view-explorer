# API密钥
OPENAI_API_KEY=你的OpenAI密钥
GOOGLE_API_KEY=你的Google密钥
GOOGLE_MAPS_MAP_ID=你的地图ID

# 域名配置
API_BASE_URL=https://api.你的域名.com
CORS_ALLOWED_ORIGINS=https://你的域名.com

# Feature Flags
ENABLE_OPENAI=true
ENABLE_GOOGLE_API=true

# 安全配置
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX_REQUESTS=50
RATE_LIMIT_WINDOW_SECONDS=30

# 会话配置
SESSION_TIMEOUT=3600
SESSION_SECURE=true

# 日志配置
LOG_LEVEL=warn 